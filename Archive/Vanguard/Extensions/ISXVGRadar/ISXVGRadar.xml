	<Window Name='RadarConfig' template='VG.Window'>
		<OnLoad>
			timedcommand 1 "rwUpdateFilters radar${rwActiveRadar}"
			timedcommand 1 UIElement[ZoomSlider@Radar Window]:SetRange[50000]
			timedcommand 1 UIElement[ZoomSlider@Radar Window]:SetValue[${Radar[radar1].Size}]
			timedcommand 1 Radar[radar1]:SetSize[${UIElement[ZoomSlider@Radar Window].Value}]
			timedcommand 1 Radar[radar2]:SetSize[${UIElement[ZoomSlider@Radar Window].Value}]
			timedcommand 1 Radar[radar3]:SetSize[${UIElement[ZoomSlider@Radar Window].Value}]
			timedcommand 1 Radar[radar4]:SetSize[${UIElement[ZoomSlider@Radar Window].Value}]
		</OnLoad>

	<template Name='radar'>
		<Font template='Default Font' />
		<ZAxisclip>0</ZAxisclip>
		<ClipRadius>1</ClipRadius>
		<ShowLabels>1</ShowLabels>
		<Rotation>1</Rotation>
		<ClipText>1</ClipText>
		<RadarSize>3750.0</RadarSize>
	</template>
	<template Name='vgradar' Template='radar'>
		<BlipTypes>
			<Bliptype Name='AggroNPC' Filename='ISXVGRadar.png' ColorKey='0' template='reddot'></BlipType>				
			<BlipType Name='Clickable' Filename='ISXVGRadar.png' ColorKey='0' Template='graydot'></BlipType>
			<BlipType Name='Corpse' Filename='ISXVGRadar.png' ColorKey='0' template='graydot'></BlipType>
			<BlipType Name='Crafting Station' Filename='ISXVGRadar.png' ColorKey='0' template='browndot'></BlipType>
			<BlipType Name='Group Member' Filename='ISXVGRadar.png' ColorKey='0' template='whitedot'></BlipType>
			<BlipType Name='Me' Filename='ISXVGRadar.png' ColorKey='0' template='lightbluedot'></BlipType>
			<BlipType Name='Merchant' Filename='ISXVGRadar.png' ColorKey='0' template='whitedot'></BlipType>
			<Bliptype Name='MyPet' Filename='ISXVGRadar.png' ColorKey='0' template='darkbluedot'></BlipType>
			<BlipType Name='NPC' Filename='ISXVGRadar.png' ColorKey='0' template='yellowdot'></BlipType>
			<Bliptype Name='NamedNPC' filename='ISXVGRadar.png' ColorKey='0' Template='pinkdot'></BlipType>
			<BlipType Name='PC' Filename='ISXVGRadar.png' ColorKey='0' template='lightgreendot'></BlipType>
			<Bliptype Name='Pet' Filename='ISXVGRadar.png' ColorKey='0' template='darkpurpledot'></BlipType>				
			<BlipType Name='QuestNPC' Filename='ISXVGRadar.png' ColorKey='0' Template='darkpurpledot'></BlipType>
			<BlipType Name='Resource' Filename='ISXVGRadar.png' ColorKey='0' template='darkgreendot'></BlipType>
			<BlipType Name='Special' Filename='ISXVGRadar.png' ColorKey='0' template='pinkdot'></BlipType>
			<BlipType Name='Trainer' Filename='ISXVGRadar.png' ColorKey='0' template='whitedot'></BlipType>
		</BlipTypes>
	</template>
	<template Name="Option Text" template="Default Font">
		<Color>FFFFFFFF</Color>
